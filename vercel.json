{
  "buildCommand": "cd web && npm install && npm run build",
  "outputDirectory": "web/dist",
  "rewrites": [
    { "source": "/auth", "destination": "/auth/index.html" },
    { "source": "/auth/(.*)", "destination": "/auth/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
