{
  "buildCommand": "yarn install --ignore-engines && npx expo export --platform web",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/auth", "destination": "/auth/index.html" },
    { "source": "/auth/(.*)", "destination": "/auth/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
