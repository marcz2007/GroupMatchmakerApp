{
  "expo": {
    "name": "GroupMatchmakerApp",
    "slug": "GroupMatchmakerApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marcz2007.GroupMatchmakerApp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.marcz2007.GroupMatchmakerApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "876034cd-48d2-4305-8754-741f1efd638f"
      }
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/react-native-vector-icons/Fonts/Ionicons.ttf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to let you set a profile picture."
        }
      ]
    ]
  }
}
